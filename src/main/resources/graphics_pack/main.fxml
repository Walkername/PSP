<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.Group?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Text?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.control.ComboBox?>
<?import javafx.collections.FXCollections?>

<?import java.lang.String?>

<?import javafx.scene.control.Button?>
<Group xmlns:fx="http://javafx.com/fxml" fx:controller="controller_pack.Controller">
    <HBox spacing="20" style="-fx-padding: 10 10 10 10">
        <VBox spacing="10">
            <ComboBox fx:id="algorithmList" onAction="#clickAlgoList" promptText="Select Filter Type">
                <items>
                    <FXCollections fx:factory="observableArrayList">
                        <String fx:value="FIR-filter" />
                        <String fx:value="IIR-filter" />
                        <String fx:value="FFT" />
                        <String fx:value="Noise Filtration" />
                        <String fx:value="Modulators" />
                    </FXCollections>
                </items>
            </ComboBox>
            <Button onAction="#addToWindow" text="Add to Queue"/>
        </VBox>
        <VBox fx:id="algorithmContent" spacing="15">
            <!--
            <Text>Choose the algorithm</Text>
            -->
        </VBox>
    </HBox>
</Group>